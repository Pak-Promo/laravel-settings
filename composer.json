{
    "name": "snippet/snippet-settings",
    "description": "Snippet settings module for Laravel projects like Snippet CMS",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "SnippetCMS",
            "email": "snippetcms@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Snippet\\SnippetSettings\\": "src"
        },
        "files": [
            "src/snippet.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Snippet\\SnippetSettings\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Snippet\\SnippetSettings\\SettingsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
